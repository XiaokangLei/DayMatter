            </view>
            <view class="right-arrow">
                 <image class="right" src="../../icons/right.png" mode="widthFix">
                 </image>
                             
            </view>

        </view>
        <view class="COVID list" hover-class="hover" data-url='COVID'  bindtap="gopage">
            <view class="text">
                 疫情监控
            </view>
            <view class="right-arrow">
                 <image class="right" src="../../icons/right.png" mode="widthFix">
                </image>  
            </view>
        </view>
    </view>
</view>
