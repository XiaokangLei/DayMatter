        <view class="content-box-img">
            <image class="hdimg" src="{{userInfo.avatarUrl}}" mode="widthFix">
            </image>
        </view>
        <view class="myname">
            {{userInfo.nickName}}
        </view>
    </view>
    <view class="toool-list">
        <view class="allBooking list"  hover-class="hover" data-url='bookingLists' bindtap="gopage">
