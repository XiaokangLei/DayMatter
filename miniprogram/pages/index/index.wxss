/* pages/indexTest/index.wxss */
page {
  width:100%;
  /* height:100%; */
  min-height:100%;
}
.button_type {
  height: 100rpx;    
  line-height :100rpx;     
  margin-left: 10%;   
  bottom: 60rpx;   
  font-size: large;      
  position: fixed;    
  width: 80%; 
  text-align: center;
  border-radius:25rpx;
}
/* 创建按钮 */
.create {
  position: fixed;
  right: 0;
  bottom: 0;
  box-sizing: border-box;
  margin: 32px;
  padding: 20rpx;
  width: 54px;
  height: 54px;
  border-radius: 50%;
  background-color: #e97d3f;
  box-shadow: rgba(0, 0, 0, 0.2) 0 3rpx 20rpx;
}