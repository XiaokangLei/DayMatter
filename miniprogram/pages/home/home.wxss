page {
  width: 100%;
  height: 100%;
  /* min-height: 100%; */
}

.date-choose-box {
  font-size: 28rpx;
  display: flex;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.date-choose-box .date-choose {
  color: #555;
  margin-left: auto;
}

.date-choose-box .date-choose .picker {
  height: 36rpx;
  display: flex;
  align-items: center;
}

.date-choose-box .date-choose .picker .dot {
  width: 8rpx;
  height: 8rpx;
  align-self: center;
  background-color: #666;
  border-radius: 50%;
  margin: 0 2rpx;
}